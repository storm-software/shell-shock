  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./components": {
      "require": {
        "types": "./dist/components/index.d.cts",
        "default": "./dist/components/index.cjs"
      },
      "import": {
        "types": "./dist/components/index.d.mts",
        "default": "./dist/components/index.mjs"
      },
      "default": {
        "types": "./dist/components/index.d.mts",
        "default": "./dist/components/index.mjs"
      }
    },
    "./components/console-builtin": {
      "require": {
        "types": "./dist/components/console-builtin.d.cts",
        "default": "./dist/components/console-builtin.cjs"
      },
      "import": {
        "types": "./dist/components/console-builtin.d.mts",
        "default": "./dist/components/console-builtin.mjs"
      },
      "default": {
        "types": "./dist/components/console-builtin.d.mts",
        "default": "./dist/components/console-builtin.mjs"
      }
    },
    "./package.json": "./package.json",
    "./types": {
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      },
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      },
      "default": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      }
    },
    "./types/plugin": {
      "require": {
        "types": "./dist/types/plugin.d.cts",
        "default": "./dist/types/plugin.cjs"
      },
      "import": {
        "types": "./dist/types/plugin.d.mts",
        "default": "./dist/types/plugin.mjs"
      },
      "default": {
        "types": "./dist/types/plugin.d.mts",
        "default": "./dist/types/plugin.mjs"
      }
    }
  },
  "types": "./dist/index.d.cts",
  "typings": "dist/index.d.mts",
  "files": ["dist/**/*"],
  "keywords": [
    "style-dictionary",
    "shell-shock",
    "shell-shock-plugin",
    "powerlines",
    "storm-software"
  ],
  "dependencies": {
    "@alloy-js/core": "catalog:",
    "@alloy-js/typescript": "catalog:",
    "@powerlines/deepkit": "catalog:",
    "@powerlines/plugin-alloy": "catalog:",
    "@powerlines/plugin-plugin": "catalog:",
    "@shell-shock/core": "workspace:*",
    "@shell-shock/plugin-theme": "workspace:*",
    "@stryke/string-format": "catalog:",
    "defu": "catalog:",
    "powerlines": "catalog:"
  },
  "devDependencies": {
    "@powerlines/plugin-deepkit": "catalog:",
    "@types/node": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
