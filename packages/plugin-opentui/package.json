{
  "name": "@shell-shock/plugin-opentui",
  "version": "0.0.2",
  "type": "module",
  "description": "A package containing a Shell Shock plugin to generate source code given a list design tokens.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/shell-shock.git",
    "directory": "packages/plugin-opentui"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.mts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": ["dist/**/*"],
  "keywords": [
    "opentui",
    "shell-shock",
    "shell-shock-plugin",
    "powerlines",
    "storm-software"
  ],
  "dependencies": {
    "@opentui/core": "catalog:",
    "@opentui/react": "catalog:",
    "@powerlines/plugin-plugin": "catalog:",
    "powerlines": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
