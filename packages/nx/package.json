{
  "name": "@shell-shock/nx",
  "version": "0.0.2",
  "description": "A Nx plugin to support Shell Shock development in Nx monorepos.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/shell-shock.git",
    "directory": "packages/nx"
  },
  "private": false,
  "main": "dist/src/index.cjs",
  "module": "dist/src/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./docs/*": "./docs/*",
    "./executors.json": "./executors.json",
    "./executors/*/schema.json": "./dist/src/executors/*/schema.json",
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.cjs"
      },
      "default": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.cjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.cjs"
      },
      "default": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.cjs"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/src/plugin/index.d.ts",
        "default": "./dist/src/plugin/index.js"
      },
      "require": {
        "types": "./dist/src/plugin/index.d.ts",
        "default": "./dist/src/plugin/index.cjs"
      },
      "default": {
        "types": "./dist/src/plugin/index.d.ts",
        "default": "./dist/src/plugin/index.cjs"
      }
    },
    "./executors": {
      "import": {
        "types": "./dist/src/executors.d.ts",
        "default": "./dist/src/executors.js"
      },
      "require": {
        "types": "./dist/src/executors.d.ts",
        "default": "./dist/src/executors.cjs"
      },
      "default": {
        "types": "./dist/src/executors.d.ts",
        "default": "./dist/src/executors.cjs"
      }
    },
    "./executors/*/untyped": {
      "import": {
        "types": "./dist/src/executors/*/untyped.d.ts",
        "default": "./dist/src/executors/*/untyped.js"
      },
      "require": {
        "types": "./dist/src/executors/*/untyped.d.ts",
        "default": "./dist/src/executors/*/untyped.cjs"
      },
      "default": {
        "types": "./dist/src/executors/*/untyped.d.ts",
        "default": "./dist/src/executors/*/untyped.cjs"
      }
    },
    "./executors/*/executor": {
      "import": {
        "types": "./dist/src/executors/*/executor.d.ts",
        "default": "./dist/src/executors/*/executor.js"
      },
      "require": {
        "types": "./dist/src/executors/*/executor.d.ts",
        "default": "./dist/src/executors/*/executor.cjs"
      },
      "default": {
        "types": "./dist/src/executors/*/executor.d.ts",
        "default": "./dist/src/executors/*/executor.cjs"
      }
    },
    "./executors/*/executor.cjs": "./dist/src/executors/*/executor.cjs",
    "./executors/*/executor.js": "./dist/src/executors/*/executor.js"
  },
  "typings": "dist/src/index.d.ts",
  "dependencies": {
    "@nx/devkit": "catalog:",
    "@powerlines/nx": "catalog:",
    "@storm-software/config": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-software/workspace-tools": "catalog:",
    "@stryke/type-checks": "catalog:",
    "defu": "catalog:",
    "powerlines": "catalog:",
    "untyped": "catalog:"
  },
  "devDependencies": {
    "@nx/workspace": "catalog:",
    "@powerlines/plugin-tsup": "catalog:",
    "@powerlines/plugin-untyped": "catalog:",
    "@storm-software/testing-tools": "catalog:",
    "@types/node": "catalog:",
    "eslint-flat-config-utils": "catalog:",
    "jsonc-eslint-parser": "catalog:",
    "powerlines": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "executors": "./executors.json"
}
