{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "out-tsc/tsconfig.tsbuildinfo",
    "jsx": "preserve",
    "jsxImportSource": "@alloy-js/core",
    "outDir": "./out-tsc",
    "sourceMap": true
  },
  "references": [],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "powerlines.config.ts",
    "powerlines.d.ts"
  ],
  "exclude": ["jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"],
  "reflection": "default",
  "reflectionLevel": "minimal"
}
